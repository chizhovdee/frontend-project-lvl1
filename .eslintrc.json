{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "fp"
    ],
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/extensions": "off",
        "fp/no-rest-parameters": "error"
    }
}
