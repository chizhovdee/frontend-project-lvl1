{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/extensions": "off",
        "no-await-in-loop": "off",
        "import/prefer-default-export": "off",
        "import/named": "off",
        "no-loop-func": "off"
    }
}
